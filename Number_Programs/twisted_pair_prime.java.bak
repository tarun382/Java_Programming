import java.util.Scanner;
class twisted_pair_prime 
{
	public static void main(String[] args) 
	{
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter the number...");
		int num = sc.nextInt();
		int num1 = num;
		int count = 0;
		for (int i=1;i<=num ;i++ )
		{
			if(num%i==0)
			{
				count++;
			}
		}
		if (count==2)
		{
			System.out.println(num+" is a primr number");
			int count1=0;
			int rev = 0;
			while (num>0)
			{
				int ld = num%10;
				
				rev = (rev*10)+ld;
				num = num/10;
			}
			for (int j=1;j<=rev ;j++ )
			{
				if (rev%j==0)
				{
					count1++;
				}
			}
			if(count1==2)
			{
				System.out.println(num1+" is a twisted pair number "+rev);
			}
			else
			{
				System.out.println(num1+" is not a twisted pair number "+rev);
			}
	
		}
		else
		{
			System.out.println(num1+" is not a prime number");
		}
	}
}
