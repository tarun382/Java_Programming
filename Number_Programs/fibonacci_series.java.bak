import java.util.Scanner;
class fibonacci_series
{
	public static void main(String[] args) 
	{
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter the nth value...");
		int n = sc.nextInt();
		int sum=0;
		int a=0;
		int b=1;
		for (int i=0;i<=n ;i++ )
		{
			System.out.println(a+" is the fibonacci number...");
			sum=a+b;
			a=b;
			b=sum;
		}
	}
}
