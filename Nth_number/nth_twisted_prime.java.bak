import java.util.Scanner;
class nth_twisted_prime 
{
	public static void main(String[] args) 
	{
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter the nth value");
		int n = sc.nextInt();
		int tcount=0
		for (int i =2; ;i++ )
		{
			int num=i;
			int count=0;
			for (int j=1;j<=num ;j++ )
			{
				if (num%j==0)
				{
					count++;
				}
			}
			if (count==2)
			{
				int rev=0;
				while (num>0)
				{
					int ld = num%10;
					rev=(rev*10)+ld;
					num=num/10;
				}
				int count1=0;
				for (int k=1;k<=rev ;k++)
				{
					if (rev%k==0)
					{
						count1++;
					}
				}
				if (count1==2)
				{
					tcount++;
				}
			}
			if (tcount==n)
					{
						System.out.println(i);
						break;

					}
		}
		
	}
}
