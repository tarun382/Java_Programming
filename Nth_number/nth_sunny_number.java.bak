import java.util.Scanner;
class nth_sunny_number 
{
	public static void main(String[] args) 
	{
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter the nth value");
		int n = sc.nextInt();
		int sunny=0;
		for (int i =1; ;i++ )
		{
			if (i==n)
			{
				int sunny=(i*i)-1;
				break;
			}
		}
		System.out.println(sunny+" is the "+n+"th sunny number");
	}
}
