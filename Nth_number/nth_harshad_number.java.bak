import java.util.Scanner;
class nth_harshad_number 
{
	public static void main(String[] args) 
	{
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter the nth value");
		int n = sc.nextInt();
		int sum=0;
		for (int i =1; ;i++ )
		{
			if (i==n)
			{
			int num = i;
			while (num>0)
			{
				int ld=num%10;
				sum=sum+ld;
				num=num/10;
			}
			if (i%sum==0)
			{
				System.out.println(i+" is the "+n+" th ");
				break;
			}
			}
			
		}
	}
}
