import java.util.Scanner;
class nth_spy_number 
{
	public static void main(String[] args) 
	{
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter the nth value");
		int n = sc.nextInt();
		int sum =0;
			int product =1;
		int count=0;
		for (int i =1; ;i++ )
		{
			int num = i;
			
			while (num>0)
			{
				int ld = num%10;
				sum = sum+ld;
				product=product*ld;
				num = num/10;
			}
			if (sum == product)
			{
				count++;
			}
			if (count==n)
			{
				System.out.println(i+" is the "+n+"th spy number");
				break;
			}
		}
	}
}
