/* WAP to print all the odd digits present in the number...*/
import java.util.Scanner;
class program10_odd_digit
{
	public static void main(String[] args) 
	{
		Scanner sc = new Scanner(System.in);
		System.out.println("enter a number...");
		int num=sc.nextInt();
		System.out.println("The odd digits of the given number "+ num +" is :- ");
		while(num>0)
		{
			int ld=num%10;
			if(ld%2==1)
			{
				System.out.println(ld+" is a odd digit in the number...");
			}
			num=num/10;
		}
	}
}