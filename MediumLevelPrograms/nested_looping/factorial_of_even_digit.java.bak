import java.util.Scanner;
class factorial_of_even_digit 
{
	public static void main(String[] args) 
	{
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter a number...");
		int num = sc.nextInt();
		while(num>0)
		{
			int ld=num%10;
			System.out.println("even digit factorials");
			if(ld%2==0)
			{
				
				int count=0;
				for(int i=1;i<=ld;i++)
				{
					
					if(ld%i==0)
					{
						count++;
					}
				}
				if(count==2)
					{
						System.out.println(ld);
					}
			}
			num=num/10;
		}
	}
}
