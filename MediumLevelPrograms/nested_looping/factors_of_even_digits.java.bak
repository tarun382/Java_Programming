import java.util.Scanner;
class factors_of_even_digits 
{
	public static void main(String[] args) 
	{
		Scanner sc = new Scanner(System.in);
		System.out.println("enter the number");
		int num = sc.nextInt();
		int temp = num;
		while(num>0)
		{
			int ld = num%10;
			
			if(ld%2==0)
			{
			System.out.println("factors of the "+ ld +" :- ");
			for(int i=1;i<=ld;i++)
			{
				if(ld%i==0)
				{
					
					System.out.println(i);
				}
			}
			}
			num=num/10;
		}
	}
}
