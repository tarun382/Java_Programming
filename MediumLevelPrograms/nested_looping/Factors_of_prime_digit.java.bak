import java.util.Scanner;
class Factors_of_prime_digit 
{
	public static void main(String[] args) 
	{
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter a number...");
		int num = sc.nextInt();
		System.out.println("prime digits of the number"+num);
		while (num>0)
		{
			int ld = num%10;
			int count=0;
			for(int i=1;i<=ld;i++)
			{
				
				if(ld%1==0)
				{
					count++;
				}
			}
			if(count==2)
				{
					System.out.println(ld);
				}
			
			num=num/10;
		}
	}
}
